<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="triangle" class="com.example.spring.Triangle" >
	<property name="points">
	<list>
		<ref bean="zeroPoint"/>
		<ref bean="point2"/>
		<ref bean="point3"/>
	</list>
	</property>
	</bean>
	<bean id="rectangle" class="com.example.spring.Rectangle" autowire="byName">
	</bean>
	<bean id="zeroPoint" class="com.example.spring.Point">
	<property name="x" value="0"/>
	<property name="y" value="0"/>
	</bean>
	<bean id="point2" class="com.example.spring.Point">
		<property name="x" value="-20"/>
		<property name="y" value="0"/>
	</bean>
	<bean id="point3" class="com.example.spring.Point">
		<property name="x" value="20"/>
		<property name="y" value="0"/>
	</bean>
	<bean id="point4" class="com.example.spring.Point">
		<property name="x" value="15"/>
		<property name="y" value="25"/>
	</bean>
	<alias name="triangle" alias="triangle-alias"/>
</beans>